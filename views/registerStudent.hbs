{{> header }}

	<div id="main_body">
		<div class="main_content">

			<h1>{{title}}</h1>
			
			<h4>{{msg}}</h4>

			<div id="loginContent">	
				<form method="POST" enctype="multipart/form-data" action="/registerStudent">
			        <div class="mdc-text-field" data-mdc-auto-init="MDCTextField">
                        <input class="mdc-text-field__input" type="text" name="id" size="24" maxlength="10">
                        <label for="id" class="mdc-floating-label">Enter Id</label>
                        <div class="mdc-line-ripple"></div> 
                    </div>
		
                    <input type="file" accept="image/*" name='img' id="image">
	
			        <button type="submit" id="submitBtn" class="mdc-button mdc-button--raised">
			        	<i class="material-icons mdc-button__icon">launch</i>
			        Register
			    	</button> 
				</form>
			</div>
			
		</div>
	</div>

    <script type="text/javascript">
   		window.mdc.autoInit();
    </script>

    {{!-- // <script src="js/studentRegister.js"></script> --}}

{{> footer-end }}